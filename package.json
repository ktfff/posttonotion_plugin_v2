{
  "name": "posttonotion",
  "displayName": "Post to notion",
  "version": "1.0.6",
  "description": "Post to Notion",
  "author": "thejoven",
  "scripts": {
    "dev": "plasmo dev",
    "build": "plasmo build --zip",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx src/",
    "cm": "cz",
    "prepare": "husky install",
    "updatePlasmo": "pnpm up -L plasmo"
  },
  "dependencies": {
    "@ant-design/cssinjs": "1.21.1",
    "@ant-design/icons": "^5.4.0",
    "@plasmohq/storage": "^1.0.5",
    "antd": "^5.13.2",
    "plasmo": "0.88.0",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "4.1.1",
    "@types/chrome": "0.0.258",
    "@types/node": "20.11.5",
    "@types/react": "18.2.48",
    "@types/react-dom": "18.2.18",
    "husky": "^9.1.5",
    "prettier": "3.2.4",
    "typescript": "5.3.3"
  },
  "manifest": {
    "permissions": [
      "storage",
      "activeTab",
      "webRequest"
    ],
    "host_permissions": [
      "https://x.com/*"
    ],
    "default_locale": "en"
  }
}
